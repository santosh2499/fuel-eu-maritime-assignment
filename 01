model Pool {
  id        String        @id @default(cuid())
  name      String
  members   PoolMember[]
  totalCredits Int
}




model PoolMember {
  id         String   @id @default(cuid())
  company    Company  @relation(fields: [companyId], references: [id])
  companyId  String
  pool       Pool     @relation(fields: [poolId], references: [id])
  poolId     String
}
